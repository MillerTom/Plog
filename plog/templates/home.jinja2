{% extends 'base.jinja2' %}
{% block content %}
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                {% for post in posts %}
                    <h4><a href="/{{ post.slug }}">{{ post.title }}</a></h4>
                    <hr>
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}